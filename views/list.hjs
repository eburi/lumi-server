<!doctype html>
<html>
	<head>
		<title>Examples - Lumi-Remote</title>

		<link rel="stylesheet" type="text/css" href="/stylesheets/style.css" />

		<script src="/javascripts/jquery-1.7.min.js" type="text/javascript"></script>
		<script src="/socket.io/socket.io.js"></script>
		<script src="/javascripts/processing-1.4.1.min.js" type="text/javascript"></script>

		<script src="/javascripts/lumi.js" type="text/javascript"></script>
		<script src="/javascripts/processingInterface.js" type="text/javascript"></script>

		<script src="/javascripts/sketches.js" type="text/javascript"></script>

		<meta charset="UTF-8" />

	</head>
	<body>
    <nav>
      <a class="menuItem current" href="/">Editor</a>
      <a class="menuItem" href="/draw.html">Draw</a>
      <a class="menuItem" href="/sketches">Sketches</a>
    </nav>
		<div id="right">
			<div id="sim">
				<div id="simMask"></div>
			</div>
		</div>
		<div id="left">
      <ul>
      {{#sketches}}
        <li class="sketch" data-sketch-name="{{name}}" data-sketch-code="{{code}}">
          <div class="sketch-title">
           <h2>{{name}}</h2>
          </div>
          <div class="sketch-desc">
            {{desc}}
          </div>
          <div class="sketch-control-box">
            <button class="sketch-control sketch-run-local">Run</button>
            <button class="sketch-control sketch-run-remote">Run Remote</button>
            <button class="sketch-control sketch-open">Open in Editor</button>
            <button class="sketch-control sketch-del">Delete</button>
          </div>
        </li>
      {{/sketches}}
      </ul>
    </div>
  </body>
</html>
